{
  "name": "docgen-electron",
  "version": "2.0.0",
  "description": "DocGenPro...",
  "main": "out/main/index.js",
  "scripts": {
    "dev": "chcp 65001 && electron-vite dev",
    "build": "electron-vite build && electron-builder",
    "preview": "electron-vite preview",
    "start": "electron-vite preview"
  },
  "build": {
    "appId": "com.docgen.pro",
    "productName": "DocGenPro",
    "directories": {
      "output": "dist_setup"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "resources/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "resources/icon.ico",
      "uninstallerIcon": "resources/icon.ico",
      "uninstallDisplayName": "卸载 DocGenPro"
    },
    "extraResources": [
      {
        "from": "templates",
        "to": "templates"
      },
      {
        "from": "rules",
        "to": "rules"
      },
      {
        "from": "resources/bin/main3.exe",
        "to": "bin/main3.exe"
      }
    ],
    "files": [
      "out/**/*"
    ]
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@element-plus/icons-vue": "^2.3.1",
    "cross-spawn": "^7.0.6",
    "element-plus": "^2.9.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "electron": "^28.2.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.0.0",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vue-tsc": "^1.8.27"
  }
}